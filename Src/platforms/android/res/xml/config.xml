<?xml version='1.0' encoding='utf-8'?>
<widget id="org.doodle.rubikDoodle" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.plugin.AdMob" />
    </feature>
    <name>DoodleRubik</name>
    <description>
        Uma homenagem ao rubik's doodle do dia 19/05/2014
    </description>
    <author email="3l70n.32@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="WebViewBounce" value="false" />
    <preference name="splashscreen" value="splash" />
    <preference name="splashScreenDelay" value="10000" />
    <icon src="img/giphy.gif" />
    <icon density="mdpi" height="57" platform="ios" src="img/giphy.gif" width="57" />
</widget>
